<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
</head>

<body>
<div id="container">
    {{range $i, $v := .}}
        <p>{{$v.Time}} 更新: <a href="{{$v.Origin}}">{{$v.Name}}</a></p>
        <div id="content">
            <ol>
                {{range $i, $v := .Articles}}
                    <li style="margin-bottom: 10px;"><a href="{{$v.Link}}">{{$v.Time}}-{{$v.Title}}</a></li>
                {{end}}
            </ol>
        </div>
    {{end}}
</div>
</div>
</body>
</html>
